(printout wsd_fp "(dir_name-file_name-rule_name-affecting_id-affected_ids-wsd_group_root_mng   " ?*wsd_dir* " wave.clp	wave0  "  ?id "  " ?id1 "  hata_jA  )" crlf))
)

