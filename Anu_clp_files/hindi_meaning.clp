 ;for MWE meaning will be assinged to the last word (single mng will be given to all words).So,by this rule we are retracting cntrl facts for remaing ids.
 (defrule mng_decided
 (declare (salience 9800))
 ?f<-(mng_has_been_decided ?id)
 ?mng<-(meaning_to_be_decided ?id)
 =>
 (retract ?mng)
 )

 ; meaning for the whole sentence
 (defrule complete_sen_trans
 (declare (salience 9500))
 (id-sen_mng ?id ?sen_mng)
 ?mng<-(meaning_to_be_decided ?id)
 =>
	(retract ?mng)
	(printout ?*hin_mng_file* "(id-HM-source  " ?id "  "?sen_mng"   Complete_sen_gdbm)" crlf)
 )
 
 ; In LWG meaning meaning for the head word is taken (as other words become tam part).
 ; For the group other than head-id this rule will assert a cntrl fact that mng_has_been_decided.
 (defrule lwg_mng
 (declare (salience 9000))
 (root-verbchunk-tam-chunkids   ?pada_head  ?  ?tam  $?ids ?pada_id)
 =>
	(bind ?len (length $?ids))
	(loop-for-count (?i 1 ?len)
		(bind ?id (nth$ ?i $?ids))
		(printout ?*hin_mng_file* "(id-HM-source  " ?id "  -   LWG)" crlf)
                (assert (mng_has_been_decided ?id))
	)
 )

 ;Ex:- Would you like a cup of tea ?
 ;Sorting the ids and assigning the mng to the last id.
 (defrule wsd_mwe_mng
 (declare (salience 8750))
 (or (affecting_id-affected_ids-wsd_group_root_mng  ?affecting_id  $?affected_ids ?grp_mng)(affecting_id-affected_ids-wsd_group_root_mng ?affecting_id  $?affected_ids ?grp_mng))
 (not (kriyA-upasarga  ?affecting_id ?id1))
 =>
 (bind $?ids (sort > (create$ ?affecting_id $?affected_ids)))
 (assert (ids-wsd_cmp_mng $?ids ?grp_mng))
 )

 ;Comparing wsd_MWE_mng and database_mng and deciding the meaning
 ;if wsd_MWE length is >= the length of th db_mng take wsd_MWE_mng else db_mng
 ;They seem to resemble each other .
 (defrule compare_wsd_and_db_MWE
 (declare (salience 8700))
 (ids-wsd_cmp_mng $?ids ?id ?mng)
 (ids-cmp_mng $?cmp_ids ?id ?mng1)
 ?f<-(meaning_to_be_decided ?id)
  =>
 (retract ?f)
 (bind ?length (length $?ids))
 (bind ?cmp_length (length $?cmp_ids))
 (if (or (> ?length ?cmp_length) (= ?length ?cmp_length)) then
  (loop-for-count (?i  1 ?length)
                (bind ?id1 (nth$ ?i $?ids))
                (printout ?*hin_mng_file* "(id-HM-source  " ?id1 "  -    WSD_compound_phrase_mng)" crlf)
                (assert (mng_has_been_decided ?id1))
  )
  (printout ?*hin_mng_file* "(id-HM-source  " ?id "  "?mng"    WSD_compound_phrase_mng)" crlf)
  else
  (loop-for-count (?i  1 ?cmp_length)
                (bind ?id1 (nth$ ?i $?cmp_ids))
                (printout ?*hin_mng_file* "(id-HM-source  " ?id1 "  -    Compound_Phrase_gdbm)" crlf)
                (assert (mng_has_been_decided ?id1))
  )
  (printout ?*hin_mng_file* "(id-HM-source  " ?id "  "?mng1"    Compound_Phrase_gdbm)" crlf))
  )

 ; WSD compound phrase mng.
 ; They seem to resemble each other .
 (defrule wsd_cmp_phrase_mng
 (declare (salience 8600))
 (ids-wsd_cmp_mng $?ids ?id ?cmp_mng)
 (root-verbchunk-tam-chunkids   ?pada_head  ?  ?tam  $?ids1)
 (test (not (member$ ?id $?ids1)));; Assuming lwg_ids (verb-verb connection) will not be part of compound phrase (noun-noun connection)
 ?mng<-(meaning_to_be_decided ?id)
 =>
 (retract ?mng)
  (bind ?length (length $?ids))
    (loop-for-count (?i  1 ?length)
                (bind ?id1 (nth$ ?i $?ids))
                (printout ?*hin_mng_file* "(id-HM-source  " ?id1 "  -    WSD_compound_phrase_mng)" crlf)
                (assert (mng_has_been_decided ?id1))
     )

        (bind ?str_len (length ?cmp_mng))
        (if (neq (str-index "[" ?cmp_mng) FALSE) then
                (bind ?index (str-index "[" ?cmp_mng))
                (bind ?str (sub-string 1 (- ?index 1) ?cmp_mng))
                (printout ?*hin_mng_file* "(id-HM-source  " ?id "  "?str"    WSD_compound_phrase_mng)" crlf)
        else
                (printout ?*hin_mng_file* "(id-HM-source  " ?id "  "?cmp_mng"    WSD_compound_phrase_mng)" crlf)
        )
 )

 ;Database compound phrase mng.
 ;I live in New York City .
 (defrule cmp_phrase_mng
 (declare (salience 8600))
 (ids-cmp_mng $?ids ?id ?cmp_mng)
 (root-verbchunk-tam-chunkids   ?pada_head  ?  ?tam  $?ids1); Assuming lwg_ids (verb-verb connection) will not be part of compound phrase (noun-noun connection)
 (test (not (member$ ?id $?ids1)))
 ?mng<-(meaning_to_be_decided ?id)
 =>
 (retract ?mng)
  (bind ?length (length $?ids))
    (loop-for-count (?i  1 ?length)
                (bind ?id1 (nth$ ?i $?ids))
                (printout ?*hin_mng_file* "(id-HM-source  " ?id1 "  -    Compound_Phrase_gdbm)" crlf)
                (assert (mng_has_been_decided ?id1))
     )

        (bind ?str_len (length ?cmp_mng))
        (if (neq (str-index "[" ?cmp_mng) FALSE) then
		(bind ?index (str-index "[" ?cmp_mng))
		(bind ?str (sub-string 1 (- ?index 1) ?cmp_mng))
		(printout ?*hin_mng_file* "(id-HM-source  " ?id "  "?str"    Compound_Phrase_gdbm)" crlf)
	else 
		(printout ?*hin_mng_file* "(id-HM-source  " ?id "  "?cmp_mng"    Compound_Phrase_gdbm)" crlf)
	)
  )


 ;WSD verb phrase mng
 ;The landlord had to back down .
 (defrule wsd_vrb_phrase_mng
 (declare (salience 8700))
 (kriyA-upasarga  ?id ?id1)
